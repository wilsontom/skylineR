    'allGenerics.R'
    'addFiles-method.R'
    'addInfo-method.R'
    'addTransitions-method.R'
    'calibrate-method.R'
    'curveOptimise.R'
    'extractCalibrants-method.R'
    'extractIS.R'
    'fitCurve.R'
    'integrate-method.R'
    'plotCalibrant-method.R'
    'predictModel.R'
    'quantify-method.R'
